import java.util.Scanner;
package com.company;

public class Main {

    public static void main(String[] args) {
        int i,fact=1;
        int number;
        Scanner s = new Scanner(System.in);
        System.out.println("Enter any number :");
        number = s.nextInt();
        for(i=1;i<=number;i++){
            fact=fact*i;
        System.out.println("Factorial of "+number+" is: "+fact);
    }}
}
